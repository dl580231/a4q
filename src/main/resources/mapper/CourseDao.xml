<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.nuc.a4q.dao.CourseDao">
 	<insert id="insertCourse" parameterType="Course">
 		insert into
 		tb_course(moderator_id,course_name,priority,create_time,last_edit_time)
 		values(#{moderator.userId},#{courseName},#{priority},#{createTime},#{lastEditTime})
 	</insert>
 	
 	<delete id="deleteCourse" parameterType="Course">
 		delete from tb_course where course_id=#{courseId}
 	</delete>
 	
 	<update id="updateCourse" parameterType="Course">
 		update tb_course set moderator_id=#{modator.userId},priority=#{priority},create_time={createTime},last_edit_time{lastEditTime} where course_id=#{courseId}
 	</update>
 	<select id="selectCourse" parameterType="Course">
 		
 	</select>
 </mapper>
 