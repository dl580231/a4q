<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.nuc.a4q.dao.CourseDao">
 	<insert id="insertCourse" parameterType="course">
 		insert into
 		tb_course(moderator_id,course_name,priority,crate_time,last_edit_time)
 		values(#{moderator.userId},#{courseName},#{priority},#{createName},#{lastEditTime})
 	</insert>
 	
 	<delete id="deleteCourse">
 		delete from tb_course where id=#{}
 	</delete>
 	
 	<update id=""></update>
 	<select id=""></select>
 </mapper>
 