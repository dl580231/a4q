<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nuc.a4q.dao.PersonInfoDao">
	<insert id="insertPersonInfo" parameterType="PersonInfo">
		insert
		into
		tb_person_info(user_name,gender,phone,email,user_type,lable,create_time,last_edit_time)
		values(#{userName},#{gender},#{phone},#{email},#{userType},#{lable},#{createTime},#{lastEditTime})
	</insert>
</mapper>